    <script>
        // íƒœê·¸ í•„í„°ë§ ê¸°ëŠ¥
        document.addEventListener('DOMContentLoaded', function() {
            const filterPills = document.querySelectorAll('.tag-filter-pill');
            const reportItems = document.querySelectorAll('.report-item');
            
            // í•„í„° í´ë¦­ ì´ë²¤íŠ¸
            filterPills.forEach(pill => {
                pill.addEventListener('click', function() {
                    const tag = this.getAttribute('data-tag');
                    
                    // í™œì„±í™”ëœ í•„í„° ì—…ë°ì´íŠ¸
                    filterPills.forEach(p => p.classList.remove('active'));
                    this.classList.add('active');
                    
                    // ë³´ê³ ì„œ í•„í„°ë§
                    reportItems.forEach(item => {
                        if (tag === 'all') {
                            item.style.display = '';
                        } else {
                            const itemTags = item.getAttribute('data-tags').split(',');
                            if (itemTags.includes(tag)) {
                                item.style.display = '';
                            } else {
                                item.style.display = 'none';
                            }
                        }
                    });
                    
                    // ëª¨ë“  ë³´ê³ ì„œê°€ ìˆ¨ê²¨ì§„ ì„¹ì…˜ì„ í™•ì¸í•˜ê³  ì²˜ë¦¬
                    document.querySelectorAll('.query-list').forEach(section => {
                        const visibleItems = section.querySelectorAll('.report-item[style=""]').length;
                        const visibleSubItems = section.querySelectorAll('.sub-list .report-item[style=""]').length;
                        
                        // ë³´ê³ ì„œ ì•„ì´í…œì´ ëª¨ë‘ ìˆ¨ê²¨ì ¸ ìˆëŠ”ì§€ í™•ì¸
                        const allHidden = (visibleItems === 0 && visibleSubItems === 0);
                        
                        // ì„¹ì…˜ ë‚´ ë³´ê³ ì„œê°€ ëª¨ë‘ ìˆ¨ê²¨ì ¸ ìˆì„ ë•Œ ì„¹ì…˜ë„ ìˆ¨ê¹€
                        section.style.display = allHidden ? 'none' : '';
                    });
                });
            });
            
            // ê¸°ë³¸ì ìœ¼ë¡œ 'ì „ì²´' í•„í„° í™œì„±í™”
            document.querySelector('.tag-filter-pill[data-tag="all"]').classList.add('active');
        });
    </script>
